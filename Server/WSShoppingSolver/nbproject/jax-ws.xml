<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="ShoppingWS">
      <implementation-class>com.polymtl.wsshoppingsolver.webservice.ShoppingWS</implementation-class>
    </service>
    <service name="ShopProductAdminWS">
      <implementation-class>com.polymtl.wsshoppingsolver.webservice.ShopProductAdminWS</implementation-class>
    </service>
  </services>
  <clients>
    <client name="ShopProductAdminWS">
      <wsdl-url>http://localhost:80/ShopProductAdminWS/ShopProductAdminWS?wsdl</wsdl-url>
      <local-wsdl-file>localhost_80/ShopProductAdminWS/ShopProductAdminWS.wsdl</local-wsdl-file>
      <package-name forceReplace="true">com.polymtl.wsshoppingsolver.webservice_client</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:80/ShopProductAdminWS/ShopProductAdminWS?wsdl</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>package</wsimport-option-name>
          <wsimport-option-value>com.polymtl.wsshoppingsolver.webservice_client</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
